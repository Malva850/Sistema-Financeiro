  object QAberturas: TFDQuery
    Connection = DM.DBFinanceiro
    SQL.Strings = (
      'SELECT ac.*, c.nm_caixa'
      ' FROM aberturas_caixas AC, CAIXAS C'
      ' WHERE AC.cd_caixa=C.cd_CAIXA'
      ' AND AC.cd_usuario=1'
      ' ORDER BY AC.dt_abertura')
