  inherited QDados: TFDQuery
    Active = True
    SQL.Strings = (
      'SELECT'
      '    CAIXAS.CD_CAIXA,'
      '    CAIXAS.NM_CAIXA,'
      '    CAIXAS.CD_USUARIO,'
      '    USUARIOS.NM_USUARIO'
      'FROM'
      '    CAIXAS'
      
        '    LEFT JOIN USUARIOS ON (USUARIOS.CD_USUARIO = CAIXAS.CD_USUAR' +
        'IO)')
