  object QAberturas: TFDQuery
    AfterScroll = QAberturasAfterScroll
    Connection = DM.DBFinanceiro
    SQL.Strings = (
      'select ac.cd_abertura_caixa,'
      '        ac.cd_caixa,'
      '        ac.dt_abertura,'
      '        ac.hr_abertura,'
      '        c.nm_caixa'
      '    from aberturas_caixas ac, caixas c'
      '    where ac.cd_caixa=c.cd_caixa'
      '    and ac.dt_fechamento is null'
      '    and ac.cd_usuario=:cdUsuario')
    Left = 508
    Top = 94
    ParamData = <
      item
        Name = 'CDUSUARIO'
        DataType = ftInteger
        ParamType = ptInput
      end>
    object QAberturasCD_ABERTURA_CAIXA: TIntegerField
      FieldName = 'CD_ABERTURA_CAIXA'
      Origin = 'CD_ABERTURA_CAIXA'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
    end
