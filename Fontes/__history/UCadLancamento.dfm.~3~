  object QSelect: TFDQuery
    Connection = DM.DBFinanceiro
    SQL.Strings = (
      'SELECT * FROM LANCAMENTOS AS LANC'
      'INNER JOIN EMPRESAS AS EMP ON LANC.CD_EMPRESA = EMP.CD_EMPRESA'
      
        'INNER JOIN TIPOS_PAGAMENTOS AS PAG ON LANC.CD_TIPO_PAGAMENTO = P' +
        'AG.CD_TIPO_PAGAMENTO')
    Left = 504
    Top = 360
  end
